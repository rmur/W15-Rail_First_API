<p id="notice"><%= notice %></p>

<h1>Listing Add Other Show Details</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Series</th>
      <th>Description</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_other_show_details.each do |add_other_show_detail| %>
      <tr>
        <td><%= add_other_show_detail.title %></td>
        <td><%= add_other_show_detail.series %></td>
        <td><%= add_other_show_detail.description %></td>
        <td><%= add_other_show_detail.image %></td>
        <td><%= link_to 'Show', add_other_show_detail %></td>
        <td><%= link_to 'Edit', edit_add_other_show_detail_path(add_other_show_detail) %></td>
        <td><%= link_to 'Destroy', add_other_show_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add other show detail', new_add_other_show_detail_path %>
